pipeline {
  agent any

  options {
    ansiColor('xterm')
  }

   stages {
       
        stage('Clear') {
           steps {
       
               sh 'npm run report:delete'
              
           }
       }
       
       stage('Dependencies') {
           steps {
       
              sh 'npx cypress install --force'
              
           }
       }
        stage('Tests API Ventes_Clients') {
           steps {
               sh ' ./integration_Ventes_Clients.sh'
  
    }
  
 }
        stage('Tests API Ventes_Countries') {
           steps {
               sh ' ./integration_Ventes_Countries.sh'
  
    }
  
 } 


}
}
