pipeline {
  agent any

  options {
    ansiColor('xterm')
  }

   stages {
       
         stage('Clear') {
           steps {
       
               sh 'npm run report:delete'
              
           }
       }
       
         stage('Dependencies') {
           steps {
       
              sh 'npx cypress install --force'
              
           }
       }
         stage('Tests API Ventes' ) {
           steps {
                sh './Executa_Ventes_Teste.sh'
   
 
    }
       
 }  
    
 }



}

