class createFamily {

  fillFamily(code) {
    cy.get("fiducial-supplier-families-selection input").click();
    debugger
    cy.getBySel('add-item-btn').click();
    cy.get('fiducial-supplier-families-form #name input').type('family_'+code);
    cy.get('fiducial-supplier-families-form textarea').type('made in Portugal.');
    cy.contains('Sauvegarder').click();
  }
}

export default new createFamily();
