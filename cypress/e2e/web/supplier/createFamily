class createFamily {

  fillFamily(name) {
    cy.get("fiducial-supplier-families-selection").click();
    cy.get("fiducial-supplier-families-selection").click();
    cy.getBySel('add-item-btn').click();
    cy.get('fiducial-supplier-families-form #name input').type(name);
    cy.get('fiducial-supplier-families-form textarea').type('made in Portugal.');
    cy.contains('Sauvegarder').click();
  }
}

export default new createFamily();
